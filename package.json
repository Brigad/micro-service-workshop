{
  "name": "workshop",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/*/database"
    ]
  },
  "scripts": {
    "_rover": "npx --yes -p @apollo/rover rover",
    "start-db": "docker run --name brigad-workshop-db -e POSTGRES_PASSWORD=mysecretpassword -d -p 5432:5432 postgres",
    "stop-db": "docker stop brigad-workshop-db"
  },
  "dependencies": {
    "@types/lodash": "4.14.170",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.27.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-import-helpers": "1.2.1",
    "eslint-plugin-prettier": "3.4.0",
    "nodemon": "2.0.7",
    "prettier": "npm:@brigad/prettier@2.6.0-dev",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.7.2"
  }
}
